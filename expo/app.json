{
  "expo": {
    "name": "react-native-firebase-app",
    "slug": "react-native-firebase",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.okamuuu.mock-samurai-news-feed",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1096488952662-qtt0ojj9lkkls0gk9riogdb95ee974st"
        }
      }
    }
  }
}
